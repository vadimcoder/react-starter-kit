{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "React starter kit for a modern single page application. Zero configuration. Ready to go. Just paste your code!",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "dist": "webpack --display-error-details --env.dist",
    "test": "jest src",
    "lint": "eslint . && eslint --no-ignore --config .eslintrc.js --config .eslintrc-test.js --ext test.js .",
    "prepush": "yarn test && yarn lint"
  },
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.24.1",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.16",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.28.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "20.0.4",
    "node-sass": "4.5.3",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-test-renderer": "15.5.4",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.15.3",
    "redux-thunk": "2.2.0",
    "sass-loader": "6.0.5",
    "style-loader": "0.18.2",
    "webpack": "2.6.1"
  },
  "devDependencies": {
    "husky": "0.13.4",
    "webpack-dev-server": "2.4.5"
  },
  "engines": {
    "node": ">=7.5.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|ico)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
