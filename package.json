{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "React starter kit for a modern single page application. Zero configuration. Ready to go. Just paste your code!",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "dist": "webpack --display-error-details --env.production",
    "dist:analyze": "webpack --display-error-details --env.production --env.analyze",
    "test": "jest src",
    "lint": "eslint . && eslint --no-ignore --config .eslintrc.js --config .eslintrc-test.js --ext test.js .",
    "prepush": "yarn test && yarn lint"
  },
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.25.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-react-css-modules": "3.1.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "chunk-manifest-webpack-plugin": "1.1.0",
    "clean-webpack-plugin": "0.1.16",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.29.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "20.0.4",
    "node-sass": "4.5.3",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-test-renderer": "15.6.1",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-logger": "3.0.6",
    "redux-saga": "0.15.4",
    "redux-thunk": "2.2.0",
    "script-ext-html-webpack-plugin": "1.8.5",
    "style-loader": "0.18.2",
    "webpack": "3.4.1",
